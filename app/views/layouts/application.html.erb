<!DOCTYPE html>
<html>
<head>
  <title>TrelloClone</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<ul>
  <% if flash[:errors] %>
    <% flash[:errors].each do |msg|  %>
      <li> <%= msg %> </li>
    <% end %>
  <% end %>
</ul>


<%= yield %>

</body>
</html>
